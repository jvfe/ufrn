@article{tesslerLargescaleDifferencesMicrobial2017,
  title = {Large-Scale Differences in Microbial Biodiversity Discovery between {{16S}} Amplicon and Shotgun Sequencing},
  author = {Tessler, Michael and Neumann, Johannes S. and Afshinnekoo, Ebrahim and Pineda, Michael and Hersch, Rebecca and Velho, Luiz Felipe M. and Segovia, Bianca T. and {Lansac-Toha}, Fabio A. and Lemke, Michael and DeSalle, Rob and Mason, Christopher E. and Brugler, Mercer R.},
  year = {2017},
